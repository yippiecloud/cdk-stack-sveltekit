{
  "name": "@yippiecloud/cdk-stack-sveltekit",
  "version": "1.0.0",
  "bin": {
    "cdk-stack-sveltekit": "bin/deploy.js"
  },
  "main": "lib/cdk-stack-sveltekit.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "example": "degit yippiecloud/template-sveltekit example && npm install --prefix example && npm run build --prefix example",
    "deploy": "npm run example && dotenv -- cdk deploy --all --require-approval never",
    "destroy": "dotenv -- cdk destroy"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.119.0",
    "@types/jest": "27.0.1",
    "@types/node": "16.7.10",
    "aws-cdk": "1.119.0",
    "jest": "27.1.0",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "typescript": "4.4.2",
    "degit": "2.8.4"
  },
  "dependencies": {
    "@aws-cdk/core": "1.119.0",
    "@aws-cdk/aws-s3": "1.119.0",
    "@aws-cdk/aws-s3-deployment": "1.119.0",
    "@aws-cdk/aws-cloudfront": "1.119.0",
    "@aws-cdk/aws-route53-targets": "1.119.0",
    "@aws-cdk/aws-route53": "1.119.0",
    "@aws-cdk/aws-certificatemanager": "1.119.0",
    "@aws-cdk/aws-ssm": "1.119.0",
    "@aws-cdk/aws-apigatewayv2-integrations": "1.119.0",
    "@aws-cdk/aws-apigatewayv2": "1.119.0",
    "@aws-cdk/aws-lambda": "1.119.0",
    "dotenv-cli": "4.0.0"
  }
}
